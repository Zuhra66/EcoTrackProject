<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityWelcomeUser">

    <TextView
        android:id="@+id/welcome_user_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/Welcome"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="welcome_user_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/Carbon_Footprint_Tracking_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/Carbon_Footprint_Tracking"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/welcome_user_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling"
        />

    <TextView
        android:id="@+id/Sustainable_lifestyle_tips_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/Sustainable_Lifestyle_Tips"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/Carbon_Footprint_Tracking_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/community_engagement_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/community_engagement"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/Sustainable_lifestyle_tips_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />
</androidx.constraintlayout.widget.ConstraintLayout>