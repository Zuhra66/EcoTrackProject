<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityWelcomeAdmin">

    <TextView
        android:id="@+id/welcome_admin_textView2"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/Welcome_Admin"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/welcome_admin_textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/User_management_textView2"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/User_management"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/welcome_admin_textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling"
 />

    <TextView
        android:id="@+id/content_management_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/Content_management"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/User_management_textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/community_engagement_textView"
        android:layout_width="269dp"
        android:layout_height="67dp"
        android:text="@string/community_engagement"
        android:textSize="24sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/content_management_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

</androidx.constraintlayout.widget.ConstraintLayout>